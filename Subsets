class Solution(object):
    def generateSubsets(self,ind,ans,current,nums,n):
        if(ind==n):
            ans.append(current[:])
            return
        current.append(nums[ind])
        self.generateSubsets(ind+1,ans,current,nums,n)
        current.pop()
        self.generateSubsets(ind+1,ans,current,nums,n)
    def subsets(self, nums):
        """
        :type nums: List[int]
        :rtype: List[List[int]]
        """
        ans=[]
        current=[]
        n=len(nums)
        ind=0
        self.generateSubsets(ind,ans,current,nums,n)
        return ans
