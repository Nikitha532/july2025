class Solution(object):
    def findMedianSortedArrays(self, nums1, nums2):
        """
        :type nums1: List[int]
        :type nums2: List[int]
        :rtype: float
        """
        mergedList = sorted(nums1+nums2)
        n = len(mergedList)
        mid = n//2
        if n % 2 == 0 :
            return float(round((mergedList[mid-1] + mergedList[mid])/2.0,5))
        else :
            return float(mergedList[mid])
